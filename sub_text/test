#include <stdlib.h>

typedef struct s_token_list{
	int					type;
	char				*token;
	struct s_token_list	*next;
}t_token_list;

void	a()
{
	system("leaks a.out");
}

void test()
{
	t_token_list *asdf = malloc(sizeof(t_token_list) * 1);
	asdf->token = malloc(sizeof(char) * 10);
}

int main()
{
	atexit(a);
	t_token_list *temp;

	test();
	exit(EXIT_SUCCESS);
	return 0;
}
